{
  "name": "@todde.tv/gltf-type-toolkit",
  "type": "module",
  "version": "1.0.0-rc.0",
  "packageManager": "pnpm@9.15.4",
  "description": "This plugin generates type-safe glTF file representations in TypeScript and optimizes the loading and bundling of models in web projects, while being bundler-agnostic (Vite, Rollup, Webpack, esbuild, Rspack, ...).",
  "author": "Thorsten Seyschab <business@todde.tv> (https://todde.tv/)",
  "contributors": [
    {
      "name": "Thorsten Seyschab",
      "email": "business@todde.tv",
      "url": "https://todde.tv/"
    },
    {
      "name": "Andreas Fehn",
      "url": "https://github.com/fehnomenal"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/toddeTV/gltf-type-toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toddeTV/gltf-type-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/toddeTV/gltf-type-toolkit/issues"
  },
  "keywords": [
    "typescript",
    "webpack",
    "web",
    "generator",
    "rollup",
    "farm",
    "three-js",
    "threejs",
    "gltf",
    "gltf2",
    "rolldown",
    "vite",
    "esbuild",
    "rspack",
    "unplugin"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./farm": {
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./gltf-loader": {
      "import": "./dist/gltf-loader.js",
      "require": "./dist/gltf-loader.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./rolldown": {
      "import": "./dist/rolldown.js",
      "require": "./dist/rolldown.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "gltf-codegen": "dist/cli.js"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md",
    "dist",
    "package.json"
  ],
  "engines": {
    "node": "22",
    "pnpm": "9"
  },
  "scripts": {
    "bin:gltf-codegen": "esno src/cli.ts",
    "build": "tsup",
    "build:post": "esno scripts/postbuild.ts",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "run-s \"lint --fix\"",
    "playground": "pnpm -C playground run dev",
    "precompile-templates": "esno scripts/precompile-templates.ts",
    "prepare": "run-s \"precompile-templates\""
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "@rspack/core": "^1",
    "astro": "*",
    "draco3d": "^1.5.7",
    "esbuild": "*",
    "rolldown": "^1.0.0-beta.3",
    "rollup": "^3 || ^4",
    "three": ">=0.170.0",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rolldown": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@node-rs/xxhash": "~1.7.6",
    "citty": "~0.1.6",
    "consola": "~3.4.0",
    "handlebars": "~4.7.8",
    "unplugin": "~2.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~3.16.0",
    "@types/node": "~22.10.7",
    "@types/three": "~0.172.0",
    "changelogen": "0.5.7",
    "eslint": "~9.18.0",
    "eslint-plugin-format": "~1.0.1",
    "eslint-plugin-perfectionist": "~4.7.0",
    "esno": "~4.8.0",
    "fast-glob": "~3.3.3",
    "npm-run-all2": "~7.0.2",
    "three": "~0.172.0",
    "three-stdlib": "~2.35.12",
    "tsup": "~8.3.5",
    "typescript": "~5.7.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "three-stdlib": "patches/three-stdlib.patch",
      "changelogen": "patches/changelogen.patch"
    }
  }
}
